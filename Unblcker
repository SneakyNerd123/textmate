    <script>
        function changeHandler(){
            var input = document.getElementById("url")
            var display = document.getElementById("display")
            display.src = input.value
        } 
    </script>
    <input placeholder="Website unblocker by a Sneaky Nerd. Unblock almost any website!MUST ADD HTTPS:// BEFORE LINK. TYPE IN FULL URL." type="text" onchange="changeHandler()" style="width:100%" name="Url_input" id="url">
    <iframe src="" style="width:100%;height:94vh" frameborder="0" id="display"></iframe>
